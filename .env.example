# Environment variables for the PR Collaboration Tool
# Copy this file to .env and fill in your actual API keys

# Google Gemini API Key for AI analysis and message generation
GEMINI_API_KEY=your-gemini-api-key-here

# SERP API Key for search functionality (replaces Ecosia scraping)
SERP_API_KEY=your-serp-api-key-here

# SERP Engine (google, bing, etc.)
SERP_ENGINE=google

# Flask configuration
FLASK_DEBUG=false
PORT=8000
